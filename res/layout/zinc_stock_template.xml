              android:background="#000000"
              android:layout_margin="1dp">

     <TableRow
             android:id="@+id/detailer_stock_tableRow1"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:padding="5dip"
             android:background="#fff"
             android:layout_margin="1dp">

         <TextView
                 android:text="10.If yes, please note the brands sold,\n stock levels, buying prices, and selling prices for each"
                 android:textColor="@color/list_main_text_color"
                 android:textSize="@dimen/list_main_text_size"
                 android:layout_span="4"/>

     </TableRow>

