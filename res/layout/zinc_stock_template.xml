                 android:text="Brands"
                 android:textColor="@color/list_main_text_color"
                 android:layout_marginRight="10dp"/>

         <TextView
                 android:text="Stock level (1x10)"
                 android:textColor="@color/list_main_text_color"
                 android:layout_marginRight="10dp"/>

         <TextView
                 android:text="Buying Price (1x10)"
                 android:textColor="@color/list_main_text_color"
                 android:layout_marginRight="10dp"/>

         <TextView
                 android:text="Selling Price (1x10)"
                 android:textColor="@color/list_main_text_color"
                 android:layout_marginRight="10dp"/>

     </TableRow>

